		 <a href="register"> <button class="btn btn-outline-primary my-2 my-sm-0" type="submit">Deconnection</button></a>
		</form>
	  </nav>

	
	<div class="container mt-5">
		<h1 class="text-center">Welcome to our companies</h1>

	
		
		<table class="table mt-5">
			<thead>
				<tr>
					<th scope="col">ID</th>
					<th scope="col">Title</th>
					<th scope="col">Country</th>
					<th scope="col">local</th>
					<th scope="col">Description</th>
					<th scope="col">Action</th>
				</tr>
			</thead>
			<tbody>
				<% data.forEach(d => { %>
					<tr>
						<th scope="row"><%= d.ID %></th>
						<td><%= d.Title %></td>
						<td><%= d.Country %></td>
						<td><%= d.local %></td>
						<td><%= d.desc %></td>
						<td>
							<a href="/edit/<%= d.ID %>" style="color: white">
				                <button type="button" class="btn btn-success">
								Edit
                </button>
              </a>
							<a href="/delete/<%= d.ID %>" style="color: white" onclick="return confirm('are u sure ?')">
                <button type="button" class="btn btn-danger">
                  Delete
                </button>
			  </a>
			  <button   class="btn btn-info"><a href="/departement/<%= d.Title %>/<%=d.ID%>" style='color: white; text-decoration: none;' onclick="return confirm('Your welcome <%=d.Title%>?')">Show Departement</a></button>
						</td>
						
					</tr>
				<% }) %>
			</tbody>
		</table>
		<a href="/add" class="btn btn-primary">Add</a>
